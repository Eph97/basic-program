##New and improved: allows unordered shock vector inputs with skips e.g. [4, -3, 1, 5, 11, -8, -1]

using Distributions
function gen_log(shock) 
    d= Logistic() ##create logistic function
    store = zeros(length(shock),2) ##initialize store
    sumstore = 0
    for i in 1:length(shock) ##for all the possible shock values
        store[i,1] = shock[i]
        store[i,2] = pdf(d,shock[i]) #generate the probability density at that point
        sumstore = sumstore+store[i,2]
    end
    freq = zeros(length(shock))
    for i in 1:size(store,1)
        freq[i] = round(Int64,100*store[i,2]/sumstore) ##round it into integers, 100 is arbitrary
    end
    return freq
end
